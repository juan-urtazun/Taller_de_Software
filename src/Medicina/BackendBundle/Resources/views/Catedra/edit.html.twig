{% extends '::backend.html.twig' %}
{% block id 'catedra' %}
{% block titulo %}
    <h1>Editar Catedra</h1>
{% endblock %}
{% block contenido %}

<form action="{{ path('catedra_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
    <div class="divinterna">  
        {{ form_widget(edit_form) }}
    </div>
    <div class="divbotones">
        <button class="btnaceptar" type="submit"></button>
        <button class="btncancelar" type="button"  onclick='window.location.href = "{{ path('tipocargo')}}"'></button>
    </div>
</form>        
{% endblock %}
